{
  "expo": {
    "name": "whatsup",
    "slug": "whatsup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/w_icon_r.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#21bfa7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/w_icon_r.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.oluwadamoo.whatsup"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
